modules = ["python-3.11"]
run = "python3 -m gunicorn -k eventlet -w 1 -b 0.0.0.0:5000 main:app"

[deployment]
deploymentTarget = "autoscale"
build = ["pip","install","-r","requirements.txt"]
run   = ["python3","-m","gunicorn","-k","eventlet","-w","1","-b","0.0.0.0:5000","main:app"]

[env]
PORT = "5000"
PYTHONUNBUFFERED = "1"
PYTHONPATH = "."

[agent]
integrations = ["javascript_paypal==1.0.0", "javascript_openai==1.0.0", "javascript_websocket==1.0.0"]
